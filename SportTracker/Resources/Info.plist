<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>UIApplicationSceneManifest</key>
	<dict>
		<key>UIApplicationSupportsMultipleScenes</key>
		<false/>
	</dict>
	<key>UIBackgroundModes</key>
	<array>
		<string>location</string>
	</array>
	<key>UILaunchScreen</key>
	<dict/>
	<key>WKBackgroundModes</key>
	<array>
		<string>workout-processing</string>
	</array>
    <!-- iOS: splash -->
    <key>UILaunchStoryboardName</key><string>LaunchScreen</string>

    <!-- iOS: permissions -->
    <key>NSHealthShareUsageDescription</key>
    <string>This app reads your running workouts from Apple Health to import them into SportTracker.</string>
    <key>NSHealthUpdateUsageDescription</key>
    <string>App saves your workouts to Health.</string>
    <key>NSLocationWhenInUseUsageDescription</key>
    <string>App uses your location to record your running route during workouts.</string>
    <key>NSLocationAlwaysAndWhenInUseUsageDescription</key>
    <string>App uses your location to record your route while the app runs in the background.</string>
</dict>
</plist>
